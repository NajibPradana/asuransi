{
    "name": "riodwanto/superduper-filament-starter-kit",
    "description": "filament starter kit",
    "type": "project",
    "keywords": [
        "filament",
        "filamentphp",
        "laravel",
        "filament starter kit",
        "laravel starter kit",
        "admin panel",
        "dashboard",
        "cms",
        "user management",
        "role-based access",
        "seo",
        "media management",
        "theme customization",
        "tall stack",
        "livewire",
        "alpinejs",
        "tailwindcss",
        "rapid development"
    ],
    "authors": [
        {
            "name": "Rio Dewanto P",
            "email": "riodp.tech@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "amidesfahani/filament-tinyeditor": "^3.0",
        "awcodes/filament-tiptap-editor": "^3.0",
        "barryvdh/laravel-dompdf": "^3.1",
        "bezhansalleh/filament-exceptions": "^2.1",
        "bezhansalleh/filament-shield": "^3.2",
        "calebporzio/sushi": "^2.5",
        "charrafimed/global-search-modal": "^3.7",
        "codeat3/blade-fluentui-system-icons": "^1.32",
        "coderflex/filament-turnstile": "^2.3",
        "codewithdennis/filament-simple-alert": "^3.0",
        "diogogpinto/filament-auth-ui-enhancer": "^1.0",
        "endroid/qr-code": "^6.0",
        "fauzie811/filament-list-entry": "^1.0",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "filament/spatie-laravel-settings-plugin": "^3.2",
        "filament/spatie-laravel-tags-plugin": "^3.2",
        "flowframe/laravel-trend": "^0.4.0",
        "guzzlehttp/guzzle": "^7.2",
        "jaocero/radio-deck": "^1.2",
        "jeffgreco13/filament-breezy": "^2.2",
        "joaopaulolndev/filament-pdf-viewer": "^1.0",
        "laravel/framework": "^11.9",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.6",
        "league/commonmark": "^2.4",
        "league/html-to-markdown": "^5.1",
        "malzariey/filament-daterangepicker-filter": "^3.4",
        "mews/purifier": "^3.4",
        "opcodesio/log-viewer": "^3.11",
        "pxlrbt/filament-excel": "^2.4",
        "pxlrbt/filament-spotlight": "^1.3",
        "riodwanto/filament-ace-editor": "^1.0",
        "rupadana/filament-api-service": "^3.4",
        "schmeits/filament-character-counter": "^1.3",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.6",
        "shuvroroy/filament-spatie-laravel-backup": "^2.2",
        "solution-forest/filament-tree": "^2.1",
        "spatie/browsershot": "^5.0",
        "spatie/laravel-data": "^4.15",
        "stechstudio/filament-impersonate": "^3.16",
        "stichoza/google-translate-php": "^5.2",
        "tomatophp/filament-media-manager": "^1.1",
        "ysfkaya/filament-phone-input": "^3.2",
        "z3d0x/filament-logger": "^0.7.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
        ,"dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
